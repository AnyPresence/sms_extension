<%= form_for [current_account, @menu_option] do |f| %>
  <small><%= f.error_messages %></small>
  <small>
    <%= f.label :name %>
    <%= f.select :name, @available_objects  %>
  </small>
  <br/>
  <small>
    <%= f.label :format %>
    <%= f.text_field :format %>
  </small>
  <%= f.submit %>
<% end %>